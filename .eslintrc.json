{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "requestAnimationFrame": true,
        "cancelAnimationFrame": true,
        "WebSocket": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    },
    "rules": {
      "import/no-extraneous-dependencies": 0
    }
}